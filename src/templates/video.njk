{% macro vimeo(videoId) %}

<div class="video">
  <iframe
    src="https://player.vimeo.com/video/{{ videoId }}" 
    loading="lazy" 
    frameborder="0" 
    webkitallowfullscreen 
    mozallowfullscreen 
    allowfullscreen
  ></iframe>
</div>
<script src="https://player.vimeo.com/api/player.js"></script>

{% endmacro %}